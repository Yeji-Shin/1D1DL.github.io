   "source": [
    "n = input()\n",
    "mid = len(n) // 2  # 자릿수 기준으로 반으로 나누기\n",
    "\n",
    "sum_left = 0\n",
    "sum_right = 0\n",
    "\n",
    "for i in range(mid):\n",
    "    sum_left += int(n[i])\n",
    "\n",
    "for i in range(mid, len(n)):\n",
    "    sum_right += int(n[i])\n",
    "    \n",
    "if sum_left == sum_right:\n",
    "    print('LUCKY')\n",
    "else: \n",
    "    print('READY')"
   ]
  }
