   "source": [
    "n = int(input())  # 동전의 개수\n",
    "data = list(map(int, input().split()))\n",
    "\n",
    "# 오름차순으로 정렬한 후에 하나씩 만들수 있는지 없는지 확인\n",
    "data.sort()\n",
    "\n",
    "# 1부터 target-1 까지의 값을 모두 만들수 있다고 가정\n",
    "target = 1\n",
    "for x in data:\n",
    "    if target < x:  # 현재 확인하는 동전의 단위가 target 이하인지 확인\n",
    "        break\n",
    "    target += x\n",
    "    \n",
    "print(target)"
   ]
  }
